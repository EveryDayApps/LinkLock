{
   "manifest_version": 3,
   "name": "LinkLock",
   "version": "1.0.0",
   "description": "LinkLock Extension",
   "options_page": "index.html",
   "action": {
      "default_icon": {
         "16": "vite.svg",
         "48": "vite.svg",
         "128": "vite.svg"
      }
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "icons": {
      "16": "vite.svg",
      "48": "vite.svg",
      "128": "vite.svg"
   },
   "permissions": [
      "webNavigation",
      "webRequest",
      "declarativeNetRequest",
      "storage",
      "tabs"
   ],
   "host_permissions": [
      "<all_urls>"
   ]
}
