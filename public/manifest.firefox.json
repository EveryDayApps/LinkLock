{
   "manifest_version": 2,
   "name": "LinkLock",
   "version": "1.0.0",
   "description": "LinkLock Extension",
   "options_page": "index.html",
   "browser_action": {
      "default_popup": "index.html",
      "default_icon": {
         "16": "vite.svg",
         "48": "vite.svg",
         "128": "vite.svg"
      }
   },
   "icons": {
      "16": "vite.svg",
      "48": "vite.svg",
      "128": "vite.svg"
   },
   "permissions": [],
   "browser_specific_settings": {
      "gecko": {
         "id": "linklock@example.com",
         "strict_min_version": "109.0"
      }
   }
}